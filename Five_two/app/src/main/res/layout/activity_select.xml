<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView02"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/back">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:paddingLeft="16dp"
        android:paddingRight="16dp">


        <ImageButton
            android:id="@+id/selectImageButton0"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/zi"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageButton
            android:id="@+id/selectImageButton1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/a"
            android:layout_below="@+id/selectImageButton0"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp" />

        <ImageButton
            android:id="@+id/selectImageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/b"
            android:layout_alignTop="@+id/selectImageButton1"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="13dp"
            android:layout_marginEnd="13dp" />

        <ImageButton
            android:id="@+id/selectImageButton3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/c"
            android:layout_below="@+id/selectImageButton1"
            android:layout_alignLeft="@+id/selectImageButton1"
            android:layout_alignStart="@+id/selectImageButton1"
            android:layout_marginTop="25dp" />

        <ImageButton
            android:id="@+id/selectImageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/d"
            android:layout_alignTop="@+id/selectImageButton3"
            android:layout_alignLeft="@+id/selectImageButton2"
            android:layout_alignStart="@+id/selectImageButton2" />

        <ImageButton
            android:id="@+id/selectImageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/e"
            android:layout_alignTop="@+id/selectImageButton6"
            android:layout_alignLeft="@+id/selectImageButton7"
            android:layout_alignStart="@+id/selectImageButton7" />

        <ImageButton
            android:id="@+id/selectImageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/f"
            android:layout_marginTop="22dp"
            android:layout_below="@+id/selectImageButton4"
            android:layout_alignLeft="@+id/selectImageButton4"
            android:layout_alignStart="@+id/selectImageButton4" />

        <ImageButton
            android:id="@+id/selectImageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/g"
            android:layout_below="@+id/selectImageButton5"
            android:layout_alignLeft="@+id/selectImageButton3"
            android:layout_alignStart="@+id/selectImageButton3"
            android:layout_marginTop="24dp" />

        <ImageButton
            android:id="@+id/selectImageButton8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/h"
            android:layout_alignTop="@+id/selectImageButton7"
            android:layout_alignLeft="@+id/selectImageButton6"
            android:layout_alignStart="@+id/selectImageButton6" />

        <ImageButton
            android:id="@+id/selectImageButton9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/i"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/selectImageButton8"
            android:layout_alignStart="@+id/selectImageButton8" />

        <ImageButton
            android:id="@+id/selectImageButton10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/j"
            android:layout_below="@+id/selectImageButton7"
            android:layout_alignLeft="@+id/selectImageButton3"
            android:layout_alignStart="@+id/selectImageButton3"
            android:layout_marginTop="24dp" />
    </RelativeLayout>
</ScrollView>






